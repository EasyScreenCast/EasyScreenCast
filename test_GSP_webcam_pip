ximagesrc \
! videoconvert \
! vp8enc min_quantizer=13 max_quantizer=20 cpu-used=5 deadline=1000000 sharpness=2 target-bitrate=10000 threads=5 \
! webmmux name=mux \
! filesink location=scr.webm

// WEBM - VP8 - DESKTOP RECORDING QUALITY 2
